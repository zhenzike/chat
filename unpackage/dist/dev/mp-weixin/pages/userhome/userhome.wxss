@font-face {
  font-family: 'iconfont';
  src: url(data:font/woff2;base64,) format("woff2"), url(data:font/woff;base64,) format("woff"), url(data:font/ttf;base64,) format("truetype");
}
.content {
  padding-top: 25px;
}
.top-bar {
  position: fixed;
  z-index: 1000;
  top: 25px;
  left: 0;
  width: 100%;
  height: 88rpx;
  background: rgba(255, 255, 255, 0);
}
.top-bar .top-bar-r {
  float: right;
  padding-top: 12rpx;
  padding-right: 32rpx;
}
.top-bar .top-bar-r image {
  width: 64rpx;
  height: 64rpx;
}
.top-bar .top-bar-l {
  float: left;
  padding-left: 32rpx;
  line-height: 88rpx;
  width: 88rpx;
}
.top-bar .top-bar-l .iconfont {
  font-family: "iconfont" !important;
  font-size: 48rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.bg {
  position: fixed;
  top: 0;
  left: 0;
  z-index: -2;
  width: 100%;
  height: 100%;
}
.bg .white-bg {
  width: 100%;
  height: 100%;
}
.bg .img-bg {
  position: absolute;
  top: -10rpx;
  left: -10rpx;
  z-index: -1;
  width: 110%;
  height: 110%;
  -webkit-filter: blur(5px);
          filter: blur(5px);
  opacity: 0.6;
}
.main {
  text-align: center;
  padding-top: 148rpx;
}
.main .user-icon {
  position: relative;
  width: 400rpx;
  height: 400rpx;
  top: 50rpx;
  margin: 0 auto;
}
.main .user-icon .sex {
  position: absolute;
  z-index: 10;
  bottom: 24rpx;
  right: 24rpx;
  width: 64rpx;
  height: 64rpx;
  border-radius: 56rpx;
}
.main .user-icon .sex image {
  padding: 16rpx;
  width: 32rpx;
  height: 32rpx;
  line-height: 64rpx;
}
.main .user-icon .icon {
  z-index: 9;
  top: 0;
  width: 400rpx;
  height: 400rpx;
  border: 6rpx solid #FFFFFF;
  border-radius: 120rpx;
}
.main .user-message {
  padding-top: 70rpx;
}
.main .user-message .name {
  font-size: 52rpx;
  color: #272832;
  font-weight: 500;
  line-height: 68rpx;
}
.main .user-message .mess {
  padding: 20rpx 120rpx;
  font-size: 28rpx;
  color: #272832;
  line-height: 48rpx;
  font-weight: 200;
}
.btn-div {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 104rpx;
  padding: 12rpx 40rpx;
  box-sizing: border-box;
}
.btn-div .btn {
  width: 684rpx;
  height: 80rpx;
  background: #7FFFD4;
  border-radius: 10rpx;
  text-align: center;
  line-height: 80rpx;
  font-size: 32rpx;
  color: white;
}
.add-friend {
  position: fixed;
  box-sizing: border-box;
  bottom: -75%;
  width: 100%;
  height: 75%;
  padding: 0 56rpx;
  background: white;
  border-radius: 40rpx 40rpx 0px 0px;
}
.add-friend .name {
  padding: 200rpx 0 40rpx;
  font-size: 52rpx;
  color: #272832;
  font-weight: 400;
  line-height: 74rpx;
}
.add-friend .add-main {
  height: 320rpx;
  background: #F3F4F6;
  border-radius: 20px;
  font-size: 32rpx;
  color: #272832;
  font-weight: 400;
  padding: 18rpx 20rpx;
  line-height: 44rpx;
}
.add-bt {
  display: flex;
  position: fixed;
  bottom: -25%;
  width: 100%;
  height: 15%;
  padding: 12rpx 40rpx;
  box-sizing: border-box;
}
.add-bt .close {
  margin: auto;
  height: 80rpx;
  width: 172rpx;
  background: #F3F4F6;
  border-radius: 10rpx;
  text-align: center;
  line-height: 80rpx;
  font-size: 32rpx;
  color: #272832;
}
.add-bt .send {
  flex: auto;
  margin: auto auto auto 40rpx;
  height: 80rpx;
  background: #7FFFD4;
  border-radius: 10rpx;
  text-align: center;
  line-height: 80rpx;
  font-size: 32rpx;
  color: white;
}


@font-face {
  font-family: 'iconfont';
  src: url(data:font/woff2;base64,) format("woff2"), url(data:font/woff;base64,) format("woff"), url(data:font/ttf;base64,) format("truetype");
}
.content {
  box-sizing: border-box;
  padding-top: 25px;
  background-color: #f3f4f6;
  height: 100vh;
}
.top-bar {
  position: fixed;
  display: flex;
  z-index: 1000;
  top: 25px;
  left: 0;
  width: 100%;
  height: 88rpx;
  background: rgba(255, 255, 255, 0.97);
  border-bottom: 1px solid rgba(39, 40, 50, 0.1);
}
.top-bar .top-bar-r {
  padding-top: 12rpx;
  padding-right: 46rpx;
}
.top-bar .top-bar-r image {
  width: 64rpx;
  height: 64rpx;
  border-radius: 20rpx;
}
.top-bar .top-bar-center {
  flex: auto;
  font-size: 40rpx;
  color: #272832;
  font-weight: 400;
  line-height: 88rpx;
  text-align: center;
}
.top-bar .top-bar-l {
  padding-left: 32rpx;
  line-height: 88rpx;
  width: 88rpx;
}
.top-bar .top-bar-l .iconfont {
  font-family: "iconfont" !important;
  font-size: 48rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.chat {
  height: 100%;
  box-sizing: border-box;
  padding-left: 32rpx;
  padding-right: 32rpx;
  padding-top: 88rpx;
}
.chat .loading {
  text-align: center;
}
.chat .loading image {
  height: 30rpx;
  width: 30rpx;
}
.chat .chat-main {
  display: flex;
  flex-direction: column;
}
.chat .msg-main {
  display: flex;
  padding: 20rpx 0;
}
.chat .msg-main .user-icon {
  flex: none;
  width: 80rpx;
  height: 80rpx;
  border-radius: 20rpx;
}
.chat .chat-content {
  flex-direction: row;
}
.chat .chat-content .chat-time {
  font-size: 24rpx;
  color: rgba(39, 40, 50, 0.3);
  line-height: 34rpx;
  padding: 20rpx 0;
  text-align: center;
}
.chat .msg-right {
  flex-direction: row-reverse;
}
.chat .msg-right .message-text {
  margin-right: 18rpx;
  background-color: #29ffa6;
  border-radius: 20rpx 0 20rpx 20rpx;
  color: #fff !important;
}
.chat .msg-right .voice {
  text-align: right;
}
.chat .msg-right .voice .voice-img {
  float: left;
  padding: 8rpx 0 0 0;
}
.chat .msg-right .message image {
  margin-right: 18rpx;
}
.chat .msg-left .message-text {
  margin-left: 18rpx;
  background-color: #fff;
  border-radius: 0 20rpx 20rpx 20rpx;
}
.chat .msg-left .voice .voice-img {
  float: right;
  padding: 8rpx 0 0 0;
}
.chat .msg-left .message image {
  margin-left: 18rpx;
}
.chat .message {
  max-width: 500rpx;
}
.chat .message .message-text {
  font-size: 32rpx;
  color: #272832;
  font-weight: 400;
  line-height: 44rpx;
  padding: 16rpx 8rpx 16rpx 16rpx;
}
.chat .message image {
  width: 400rpx;
  border-radius: 20rpx;
}
.chat .message .voice .voice-img {
  width: 32rpx;
  height: 32rpx;
}
.chat .message-voice {
  min-width: 150rpx;
}

